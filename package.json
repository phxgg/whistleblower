{
  "name": "whistleblower",
  "version": "1.0.0",
  "description": "Discord bot that keeps track of everything happening on your server.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "register": "node scripts/register.mjs",
    "delete": "node scripts/delete.mjs",
    "bot": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "ncu:check": "npx npm-check-updates",
    "ncu:update": "npx npm-check-updates -u",
    "bump-deps": "npm run ncu:update && npm install && git add . && git commit -m \"chore: bump deps\""
  },
  "author": "phxgg",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "discord-api-types": "^0.38.37",
    "discord.js": "^14.25.1",
    "form-data": "^4.0.5",
    "mongodb": "^7.0.0",
    "mongoose": "^9.1.2",
    "nodemon": "^3.1.11",
    "redis": "^5.10.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "prettier": "3.7.4"
  }
}
